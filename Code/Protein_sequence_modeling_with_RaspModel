 with {len(edits)} mutations.'\n","    \n","    op, pos, _ = edits[0]\n","    assert op != 'insert', 'Found insertion.'\n","    \n","    if op == 'replace':\n","        row['wt'] = wtype[pos]\n","        row['mt'] = seq[pos]\n","        row['pos'] = pos + 1\n","\n","    if op == 'delete':\n","        row['wt'] = wtype[pos]\n","        row['mt'] = '*'\n","        row['pos'] = pos + 1\n","        \n","    return row "],"metadata":{"id":"8EJpgq-MR0Kl"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["test_wo_wtype = test.drop(WTYPE_ID)\n","test_wo_wtype = test_wo_wtype.apply(get_mutation, wtype=wtype, axis=1)\n","\n","test_wo_wtype.head()"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":238},"id":"TERsBcHjTcG3","executionInfo":{"status":"ok","timestamp":1671418369820,"user_tz":480,"elapsed":3870,"user":{"displayName":"ksd icecream","userId":"11380386624350471835"}},"outputId":"385a1940-4f92-4230-accd-851ecd004eed"},"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/plain":["                                         protein_sequence  pH data_source wt  \\\n","seq_id                                                                         \n","31390   VPVNPEPDATSVENVAEKTGSGDSQSDPIKADLEVKGQSALPFDVD...   8   Novozymes  L   \n","31391   VPVNPEPDATSVENVAKKTGSGDSQSDPIKADLEVKGQSALPFDVD...   8   Novozymes  L   \n","31392   VPVNPEPDATSVENVAKTGSGDSQSDPIKADLEVKGQSALPFDVDC...   8   Novozymes  L   \n","31393   VPVNPEPDATSVENVALCTGSGDSQSDPIKADLEVKGQSALPFDVD...   8   Novozymes  K   \n","31394   VPVNPEPDATSVENVALFTGSGDSQSDPIKADLEVKGQSALPFDVD...   8   Novozymes  K   \n","\n","       mt  pos  \n","seq_id          \n","31390   E   17  \n","31391   K   17  \n","31392   *   17  \n","31393   C   18  \n","31394   F   18  "],"text/html":["\n","  <div id=\"df-bbbaa2db-cc12-4169-8c40-4d0c0aa167d1\">\n","    <div class=\"colab-df-container\">\n","      <div>\n","<style scoped>\n","    .dataframe tbody tr th:only-of-type {\n","     